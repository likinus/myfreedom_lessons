    <script>
    var arr = [5, 4, 3, -3, -10, -1, 8, -20, 0];
      function findPositiveNumbers(arr){
          let result = [];
            for(let element of arr){
                if (element >= 0 ){
                  result.push(element);
                }
            }
           return result;
        }
        console.log(findPositiveNumbers(arr));
        
         function findPositiveNumbers(arr){
            let positiveArr = arr.filter(function(number){
                return number >= 0;
                                         });
                return positiveArr;
        }
            console.log(findPositiveNumbers(arr));
</script>
